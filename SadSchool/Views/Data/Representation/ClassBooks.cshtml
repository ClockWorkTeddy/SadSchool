@model List<string>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Document</title>
</head>
<body>
    <div class="container-fluid">
        <div class="container scroll-sad" style="max-width: 800px; max-height: 600px">
            @for(int i = 0; i < Model.Count / 3; i++)
            {
                <div class="row justify-content-center" style="margin-top:30px">
                    @for(int j = 0; j < 3; j++)
                    {
                        <div class="col-4 d-flex justify-content-center">
                            <a class="btn btn-sad btn-main-sad blinker-sad" role="button"
                               asp-controller="ClassBooks" asp-action="ClassBook" asp-route-className="@Model[i * 3 + j]">@Model[i * 3 + j]</a>
                        </div>
                    }
                </div>
            }
        </div>
    </div>
    <script src="/js/bootstrap.bundle.min.js"></script>
</body>
</html>